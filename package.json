{
  "name": "modern-adminx",
  "version": "1.9.1",
  "private": true,
  "description": "modern-adminx open source management system",
  "license": "MIT",
  "homepage": "https://github.com/fxzer/modern-adminx.git",
  "author": {
    "name": "fxzer",
    "email": "3010099292@qq.com",
    "url": "https://github.com/fxzer"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fxzer/modern-adminx.git"
  },
  "bugs": {
    "url": "https://github.com/fxzer/modern-adminx.git/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:test": "vue-tsc && vite build --mode test",
    "report": "vue-tsc && vite build --report",
    "build:pro": "vue-tsc && vite build --mode production",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "release": "standard-version",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "commit": "git add -A && czg && git push",
    "clean:cache": "rimraf node_modules && rimraf .eslintcache && pnpm install"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:fix",
    "commit-msg": "pnpm run commitlint",
    "pre-push": "pnpm run release"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@pureadmin/utils": "^1.9.10",
    "@vueuse/core": "^10.4.1",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.3.14",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "print-js": "^1.6.0",
    "qs": "^6.11.2",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.4.1",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@antfu/ni": "^0.21.8",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/json": "^2.2.119",
    "@plugin-web-update-notification/vite": "^1.6.5",
    "@types/md5": "^2.3.2",
    "@types/nprogress": "^0.2.1",
    "@types/qs": "^6.9.8",
    "@types/sortablejs": "^1.15.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@unocss/eslint-config": "^0.56.1",
    "@unocss/preset-rem-to-px": "^0.56.1",
    "@unocss/reset": "^0.56.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "autoprefixer": "^10.4.16",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.30",
    "postcss-html": "^1.5.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.68.0",
    "simple-git-hooks": "^2.9.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-setup-extend-plus": "^1.0.0",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.7.38",
    "vite-plugin-pwa": "^0.16.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vue-tsc": "^1.8.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:fix",
    "commit-msg": "pnpm run commitlint",
    "pre-push": "pnpm run release"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
