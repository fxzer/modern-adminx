{
  "name": "modern-adminx",
  "version": "1.4.2",
  "private": true,
  "description": "modern-adminx open source management system",
  "license": "MIT",
  "homepage": "https://github.com/fxzer/modern-adminx.git",
  "author": {
    "name": "fxzer",
    "email": "3010099292@qq.com",
    "url": "https://github.com/fxzer"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fxzer/modern-adminx.git"
  },
  "bugs": {
    "url": "https://github.com/fxzer/modern-adminx.git/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:test": "vue-tsc && vite build --mode test",
    "build:pro": "vue-tsc && vite build --mode production",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "release": "standard-version",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "commit": "git add -A && czg && git push",
    "clean:cache": "rimraf node_modules && rimraf .eslintcache && pnpm install"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:fix",
    "commit-msg": "pnpm run commitlint",
    "pre-push": "pnpm run release"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@iconify-json/clarity": "^1.1.9",
    "@vueuse/core": "^10.3.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "driver.js": "^0.9.7",
    "echarts": "^5.4.3",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.3.4",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "print-js": "^1.6.0",
    "qs": "^6.11.2",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify-json/ep": "^1.1.12",
    "@iconify-json/material-symbols": "^1.1.50",
    "@iconify-json/mdi": "^1.1.52",
    "@iconify-json/solar": "^1.1.1",
    "@types/md5": "^2.3.2",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@unocss/eslint-config": "^0.55.7",
    "@unocss/preset-rem-to-px": "^0.55.7",
    "@unocss/reset": "^0.55.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "cz-git": "^1.7.0",
    "czg": "^1.7.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.64.2",
    "simple-git-hooks": "^2.9.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.1.6",
    "unocss": "^0.55.7",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-setup-extend-plus": "^1.0.0",
    "vite": "^4.4.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.8.8"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
